-- add a review (all values besides book_id and username will be generated by user) --
insert into netbooks.reviews (rating, details, books_id, users_username) values (rating, details, books_id, users_username);

-- add a user (all values provided by user)--
insert into netbooks.users (username, password, fname, lname, birthdate, address, city, state, zipcode, subscription) values ((username, password, fname, lname, birthdate, address, city, state, zipcode, subscription);

-- add a waitlist entry (both values are provided by the application) --
insert into netbooks.waitlist (Users_username, books_id) values (Users_username, books_id);
