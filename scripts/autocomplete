//NOT ACTUALLY THE SCRIPT FILE, GUIDE FOR WHEN WE IMPLEMENT
//FOR SERVLET SIDE
		//this should be in the Search servlet
		import com.google.gson.Gson;
		//jar for Gson is downloaded from here: http://www.java2s.com/Code/Jar/g/Downloadgson222jar.htm

		//create String[] of genres, authors, and book titles, called array
		String[] array;
		Gson g = new Gson();
		String gstring = g.toJson(array);
		response.setContentType("text/html");
		response.getWriter().write(gstring);

//FOR CLIENT SIDE
	
		$(document).ready(auto);

		function auto(){
			$.ajax({url:"Search", success:addArr, dataType:'json'});
			//retrieves array from Search servlet
		}

		function addArr(list){
			//input is id of the search bar
			$("#input").autocomplete({source:list});	
		}
